Switch statement for numbers

import java.util.Scanner;
class Main
{
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        switch(n)
        {
            case 1:
                System.out.println("Monday");
                break;
            case 2:
                System.out.println("Tuesday");
                break;
            case 3:
                System.out.println("Wednesday");
                break;
            case 4:
                System.out.println("Thursday");
                break;
            case 5:
                System.out.println("Friday");
                break;
            case 6:
                System.out.println("Saturday");
                break;
            case 7:
                System.out.println("Sunday");
                break;
            default:
                System.out.println("invalid value");
        }
       
       
    }    
}

//Switch statement for String

import java.util.Scanner;
class Main
{
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        String s = sc.nextLine();
        switch(s)
        {
            case "Ahad":
                System.out.println("Monday");
                break;
            case "Khan":
                System.out.println("Tuesday");
                break;
            case "Ali":
                System.out.println("Wednesday");
                break;
            
            default:
                System.out.println("invalid value");
        }
       
       
    }    
}





//Switch statement new syntax

import java.util.Scanner;
class Main
{
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        String s = sc.nextLine();
        switch(s)
        {
            case "Ahad" -> System.out.println("Monday");
                
            case "Khan" -> System.out.println("Tuesday");
               
            case "Ali" -> System.out.println("Wednesday");
                
            default -> System.out.println("invalid value");
               
        }
       
       
    }    
}

// Online Java Compiler
// Use this editor to write, compile and run your Java code online
import java.util.Scanner;
class Main
{
    public static void main(String args[])
    {
        
        String s = "Ahad";
        String result = "";
        switch(s)
        {
            case "Ahad" -> result = "6am";
                
            case "Khan" -> result = "7am";
               
            case "Ali" -> result = "8am";
                
            default -> result = "invalid value";
               
        }
        System.out.println(result);
       
       
    }    
}
